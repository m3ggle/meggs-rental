generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model offers {
  offerId    String    @id @db.Uuid
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  name       String?
  like_count Int?      @db.SmallInt
}
